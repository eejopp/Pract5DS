<Window x:Class="Pract4_DB.WndManager"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pract4_DB"
        mc:Ignorable="d"
        Title="WndManager" Height="450" Width="800">
    <UniformGrid Rows="2" Columns="3">
        <Button x:Name="btnSuppliers" Content="Suppliers" Click="btnSuppliers_Click"/>
        <Button x:Name="btnInventory" Content="Inventory" Click="btnInventory_Click"/>
        <Button x:Name="btnDiscounts" Content="Discouns" Click="btnDiscounts_Click"/>
        <Button x:Name="btnProducts" Content="Products" Click="btnProducts_Click"/>
    </UniformGrid>
</Window>
